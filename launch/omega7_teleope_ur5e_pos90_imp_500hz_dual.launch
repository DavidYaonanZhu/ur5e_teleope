<?xml version="1.0"?>
<launch>
   <arg name="use_tool" default="true" />
   <arg name="force_scale" default="0.1" />
   <arg name="work_space_scale" default="1.0" />

<!-- launch rviz using my configuration file -->
  <node name="rviz" pkg="rviz" type="rviz" args="-d $(find ur5e_teleope)/config/marker_teleope.rviz"/>



<!-- launch imp_controller solver -->
  <node pkg="ur5e_teleope" name="ur_imp" type="ur_imp" output="screen"/>

<!-- launch trac_ik solver -->
  <node pkg="ur5e_teleope" name="ur5e_pos90_15cmtool_omega_ik_imp_500hz.py" type="ur5e_pos90_15cmtool_omega_ik_imp_500hz.py" output="screen"/>

</launch>
